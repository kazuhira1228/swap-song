<header class="header">
  <h1>swap-song</h1>
  <div class="nav">
    <ul class="lists">
      <%# <li><%= link_to 'TOP', root_path, class: "list" %>
      <%# <li><%= link_to 'ログイン', new_user_session_path, class: "list" %>
      <%# <li><%= link_to '新規登録'%> 
    </ul>
  </div>
</header>



<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

<body>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="form">
    <h2 class="form-title">ログイン</h2>

    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>

    <div class="field">
      <%= f.label :パスワード %><br />
      <%= f.password_field :password, autocomplete: "current-password" %>
    </div>

    <br />
  </div>
  <div class="actions">
    <%= f.submit "登録", class: "devise-btn" %>
  </div>

</body>

<% end %>
